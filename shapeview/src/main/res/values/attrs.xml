<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--shape类型 GradientDrawable.RECTANGLE, GradientDrawable.OVAL, GradientDrawable.LINE, GradientDrawable.RING  0~3 -->
    <attr name="sv_shape">
        <!-- 矩形 -->
        <enum name="rectangle" value="0" />
        <!-- 圆形 -->
        <enum name="oval" value="1" />
        <!-- 线条 -->
        <enum name="line" value="2" />
        <!-- 圆环 -->
        <enum name="ring" value="3" />
    </attr>
    <!--填充色-->
    <attr name="sv_solidColor" format="color|reference" />
    <!--边框色-->
    <attr name="sv_strokeColor" format="color|reference" />

    <!--渐变颜色-->
    <attr name="sv_startColor" format="color|reference" />
    <attr name="sv_centerColor" format="color|reference" />
    <attr name="sv_endColor" format="color|reference" />
    <!--openSelector开启时，选中时的渐变颜色-->
    <attr name="sv_startSelectColor" format="color|reference" />
    <attr name="sv_centerSelectColor" format="color|reference" />
    <attr name="sv_endSelectColor" format="color|reference" />
    <!--渐变方向 GradientDrawable.Orientation 0~7 -->
    <attr name="sv_orientation" >
        <enum name="TOP_BOTTOM" value="0" />
        <enum name="TR_BL" value="1" />
        <enum name="RIGHT_LEFT" value="2" />
        <enum name="BR_TL" value="3" />
        <enum name="BOTTOM_TOP" value="4" />
        <enum name="BL_TR" value="5" />
        <enum name="LEFT_RIGHT" value="6" />
        <enum name="TL_BR" value="7" />
    </attr>
    <!--//渐变类型 支持 线性渐变，放射性渐变，扫描式渐变  0~2 -->
    <attr name="sv_gradientType">
        <!-- 线性渐变 -->
        <enum name="linear" value="0" />
        <!-- 放射性渐变 -->
        <enum name="radial" value="1" />
        <!-- 扫描式渐变 -->
        <enum name="sweep"  value="2" />
    </attr>
    <!--渐变半径. 只有当渐变类型设置为 RADIAL_GRADIENT  时，半径才有效。 -->
    <attr name="sv_gradientRadius" format="float" />

    <!--边框宽度-->
    <attr name="sv_strokeWidth" format="dimension" />
    <!--圆角弧度-->
    <attr name="sv_radius" format="dimension" />
    <!--虚线边框宽度-->
    <attr name="sv_dashWidth" format="dimension" />
    <!--虚线边框间隙-->
    <attr name="sv_dashGap" format="dimension" />
    <!--四个角的圆角弧度-->
    <attr name="sv_topLeftRadius" format="dimension" />
    <attr name="sv_topRightRadius" format="dimension" />
    <attr name="sv_bottomLeftRadius" format="dimension" />
    <attr name="sv_bottomRightRadius" format="dimension" />
    <!--是否使用Selector选择器-->
    <attr name="sv_openSelector" format="boolean" />
    <!--使用选择器时的字体色-->
    <attr name="sv_textNormalColor" format="color|reference" />
    <attr name="sv_textSelectColor" format="color|reference" />
    <!--选中填充色-->
    <attr name="sv_solidSelectColor" format="color|reference" />
    <!--选中边框色-->
    <attr name="sv_strokeSelectColor" format="color|reference" />

